.item {
    margin: var(--cell-vertical-padding,10px) var(--cell-horizontal-padding,16px);
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1px solid #eee;
    margin-bottom: 0;
    padding-bottom:var(--cell-vertical-padding,10px);
}

.item:first-child{
    border-bottom: 0;
}

.item:last-child{
    border-bottom: 0;
}

.item .icon {
    width: 80rpx;
    height: 80rpx;
    background: #ddd;
    border-radius: 50%;
    overflow: hidden;
}

.item .title {
    font-size: 28rpx;
    color: #333333;
}

.item .icon image {
    width: 80rpx;
    height: 80rpx;
}

.item .right {
    font-size: 30rpx;
    color: #999999;
}

.item .right text{
    display: inline-block;
    padding-right: 10rpx;
}
.item .right image {
    width: 20rpx;
    height: 20rpx;
}

.line-20 {
    background: #F6F6F6;
    width: 100%;
    height: 20rpx;
    margin-bottom: 0;
}

.definition .title{
    font-size: 30rpx;
    padding: 29rpx 0rpx 20rpx 40rpx;
    font-weight: bold;
}

.pup-list{
    width: 500rpx;
    padding: 20rpx;
}

.pup-list .list-item{
    padding: 20rpx;
    font-size: 30rpx;
    display: flex;
    justify-content: space-between;
}

.pup-list .list-item:first-child{
    padding-top: 0rpx;
}

.pup-list .list-item .radio{
    transform: scale(0.7);
}