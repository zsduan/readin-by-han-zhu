.clearfix ::after {
  clear: both;
  display: block;
  zoom: 1;
  content: "";
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.line-20 {
  width: 100%;
  height: 20rpx;
  background: #F9F9F9;
  border-radius: 18px;
  margin-bottom: 20rpx;
}

.titles {
  width: 90%;
  text-align: center;
  padding-top: 60rpx;
  height: 50rpx;
  font-size: 40rpx;
  font-family: MF LangQian (Noncommercial);
  color: #F16E22;
  position: fixed;
  background: #fff;
  top: 0;
  z-index: 999;
  padding-bottom: 10rpx;
}

